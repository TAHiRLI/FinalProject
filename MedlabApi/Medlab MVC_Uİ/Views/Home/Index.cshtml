            @foreach (var slide in Model.Sliders)
            {
                
            <!-- slide  -->
            <div class="mainSlider_item d-flex justify-content-center">
                <div class="holder">
                    <img src="/Assets/Uploads/Sliders/@slide.ImageUrl"
                         alt=""
                         class="mainSlider_item_image" />
                </div>
                <div class="mainSlider_item_content d-flex flex-column align-items-center justify-content-center position-absolute">
                    <p class="mainSlider_item_content_title p-0">
                        @slide.Title
                    </p>
                    <p class="mainSlider_item_content_desc">
                       @slide.Desc
                    </p>
                    <a href="@slide.BtnUrl"
                       class="mainSlider_item_content_button rounded-pill my-custom-btn p-0 py-2 px-3 mt-lg-5 px-lg-4 py-lg-3">
                       @slide.BtnText <i class="las la-arrow-right"></i>
